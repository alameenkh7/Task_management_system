{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "monorepo": true,
  "projects": {
    "nashville": {
      "type": "application",
      "root": "apps/nashville",
      "entryFile": "main",
      "sourceRoot": "apps/nashville/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nashville/tsconfig.build.json",
        "webpack": true
      }
    },
    "gallatin": {
      "type": "application",
      "root": "apps/gallatin",
      "entryFile": "main",
      "sourceRoot": "apps/gallatin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gallatin/tsconfig.build.json",
        "webpack": true
      }
    },
    "ashland": {
      "type": "application",
      "root": "apps/ashland",
      "entryFile": "main",
      "sourceRoot": "apps/ashland/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ashland/tsconfig.build.json",
        "webpack": true
      }
    }
  }
}
